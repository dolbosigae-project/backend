<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gae.mapper.PHMapper">

    <!-- ResultMap -->
    <resultMap type="com.gae.dto.PHDTO" id="pharmacy">
        <id column="PH_id" property="PH_ID" />
        <result column="ph_name" property="PH_NAME" />
        <!-- 나머지 필드도 추가 -->
    </resultMap>

    <select id="selectAllPh" resultMap="pharmacy">
        select * from PH
    </select>

    <select id="selectph" parameterType="int" resultMap="pharmacy">
        select * from PH where ph_id = #{id}
    </select>

</mapper>
